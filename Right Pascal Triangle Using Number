package Javais;

public class Duplicate {
	    public static void main(String[]args){
	        int n=5;
	        
	    for(int i=1;i<n;i++){
	        for(int j=1;j<=i;j++){
	            System.out.print(j);
	        }
	        System.out.println();
	    }
	    for(int i=1;i<=n;i++){
	        for(int j=i,p=1;j<=n;j++,p++){
	            System.out.print(p);
	        }
	        System.out.println();
	    }
	    }
}
